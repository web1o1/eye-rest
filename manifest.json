{
  "name": "Eye Rest",
  "version": "1.0",
  "description": "An extension to rest your eyes every 20 minutes for 20 seconds.",
  "permissions": ["activeTab", "alarms", "declarativeContent", "storage", "tabs"],
  "background": {
    "scripts": ["helpers.js", "background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/lookaway16.png",
      "32": "images/lookaway32.png",
      "48": "images/lookaway48.png",
      "128": "images/lookaway128.png"
    }
  },
  "icons": {
    "16": "images/lookaway16.png",
    "32": "images/lookaway32.png",
    "48": "images/lookaway48.png",
    "128": "images/lookaway128.png"
  },
  "manifest_version": 2
}